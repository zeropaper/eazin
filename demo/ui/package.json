{
  "name": "eazin-demo-ui",
  "version": "0.0.1-rc.4",
  "description": "Demo ui made with the EAZIN SDK",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zeropaper/eazin.git",
    "directory": "demo/ui"
  },
  "main": "src/index.js",
  "scripts": {
    "prelint": "npm run copy-eazin-ui",
    "lint": "eslint --quiet --ext jsx,js src",
    "prelint:fix": "npm run copy-eazin-ui",
    "lint:fix": "eslint --quiet --fix --cache --format codeframe --ext jsx,js src",
    "prebuild": "npm run copy-eazin-ui",
    "build": "webpack --mode production",
    "postbuild": "node ./scripts/fix-html.js && node ./scripts/fix-manifest.js",
    "demo": "",
    "copy-eazin-ui": "cp -rf ../../packages/ui/dist node_modules/eazin-ui/ && cp -rf ../../packages/ui/package.json node_modules/eazin-ui/",
    "dev": "npm-run-all --parallel devcopy devbuild",
    "devcopy": "nodemon -d 0.5 -w ../../packages/ui/dist -x \"npm run copy-eazin-ui\"",
    "devbuild": "webpack-dev-server --mode development",
    "test": "jest"
  },
  "keywords": [
    "eazin",
    "demo",
    "ui"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@hot-loader/react-dom": "^16.11.0",
    "@material-ui/core": "^4.1.3",
    "@material-ui/icons": "^4.2.1",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "eazin-ui": "file:../../packages/ui",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.11.0",
    "react-hot-loader": "^4.11.1",
    "react-redux": "^7.1.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.7.0",
    "@neutrinojs/airbnb": "^9.0.0-rc.5",
    "app-manifest-webpack-plugin": "^1.2.0",
    "webpack-dev-server": "^3.9.0",
    "workbox-webpack-plugin": "^4.3.1"
  }
}
