{
  "name": "eazin-demo-ui",
  "version": "0.0.1-rc.0",
  "description": "Demo ui made with the EAZIN SDK",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zeropaper/eazin.git",
    "directory": "demo/ui"
  },
  "main": "src/index.js",
  "scripts": {
    "prelint": "npm run copy-eazin-ui",
    "lint": "eslint --quiet --ext jsx,js src",
    "prelint:fix": "npm run copy-eazin-ui",
    "lint:fix": "eslint --quiet --fix --cache --format codeframe --ext jsx,js src",
    "prebuild": "npm run copy-eazin-ui",
    "build": "webpack --mode production",
    "demo": "",
    "copy-eazin-ui": "cp -rf ../../packages/ui/dist node_modules/eazin-ui/ && cp -rf ../../packages/ui/package.json node_modules/eazin-ui/",
    "dev": "npm-run-all --parallel devcopy devbuild",
    "devcopy": "nodemon -d 0.5 -w ../../packages/ui/dist -x \"npm run copy-eazin-ui\"",
    "predevbuild": "npm run copy-eazin-ui",
    "devbuild": "webpack-dev-server --mode development",
    "test": "jest"
  },
  "keywords": [
    "eazin",
    "demo",
    "ui"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@hot-loader/react-dom": "^16.11.0",
    "@material-ui/core": "^4.1.3",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.1.2",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "classnames": "^2.2.6",
    "core-js": "^3.4.2",
    "eazin-ui": "*",
    "history": "^4.9.0",
    "informed": "^2.11.15",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.11.1",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "socket.io-client": "^2.3.0",
    "typeface-roboto": "0.0.54"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.7.0",
    "@neutrinojs/airbnb": "^9.0.0-rc.5",
    "@neutrinojs/react": "^9.0.0-rc.5",
    "app-manifest-webpack-plugin": "^1.2.0",
    "eslint": "^6.6.0",
    "eslint-plugin-jest": "^23.0.4",
    "jest": "^24.9.0",
    "neutrino": "^9.0.0-rc.5",
    "nodemon": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "workbox-webpack-plugin": "^4.3.1"
  }
}
