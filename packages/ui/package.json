{
  "name": "eazin-ui",
  "version": "0.0.1-rc.5",
  "description": "EAZIN UI",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zeropaper/eazin.git",
    "directory": "packages/ui"
  },
  "scripts": {
    "lint": "eslint --quiet --ext jsx,js src test",
    "lint:fix": "eslint --quiet --fix --cache --format codeframe --ext jsx,js src test",
    "_copy-dist-to-demo": "echo \"******* nonononon *******\"",
    "copy-dist-to-demo": "mkdir -p ../../demo/node_modules/eazin-ui && cp -rf dist ../../demo/node_modules/eazin-ui/dist && cp -rf package.json ../../demo/node_modules/eazin-ui",
    "build": "webpack --mode production",
    "postbuild": "npm run copy-dist-to-demo",
    "dev": "npm-run-all --parallel devstart devbuild",
    "devstart": "webpack-dev-server --mode development",
    "devbuild": "nodemon -e js,jsx -w src -w .neutrinorc.js -x \"npx rimraf dist/* && npm run build\"",
    "storybook": "start-storybook -p 5006",
    "test": "echo \"Run tests from root\" && exit 1"
  },
  "peerDependencies": {
    "history": "*",
    "prop-types": "*",
    "redux-devtools-extension": "*",
    "@material-ui/core": "*",
    "react-hot-loader": "*",
    "react-redux": "*",
    "typeface-roboto": "*",
    "react-dom": "*",
    "redux": "*",
    "react": "*",
    "faker": "*"
  },
  "devDependencies": {
    "eazin-2fa": "file:../2fa",
    "eazin-activities": "file:../activities",
    "eazin-clients": "file:../clients",
    "eazin-docs": "file:../docs",
    "eazin-groups": "file:../groups",
    "eazin-kitchen-sink": "file:../kitchen-sink",
    "eazin-landing": "file:../landing",
    "eazin-mailer": "file:../mailer",
    "eazin-organisations": "file:../organisations",
    "eazin-repositories": "file:../repositories",
    "eazin-users": "file:../users",
    "eazin-ws": "file:../ws"
  },
  "browserslist": [
    "last 3 version"
  ],
