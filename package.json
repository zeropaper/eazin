{
  "name": "eazinpublishingtest",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "npm": "^6.13.0",
    "node": "^12.13.0"
  },
  "scripts": {
    "prelerna:publish": "npm test && git add dist && git commit --no-verify --allow-empty -m \"core(dist) update dist builds\"",
    "lerna:publish": "lerna publish",
    "build:components:dev": "nodemon -e js,jsx -w './packages/*/src/ui/*' -x \"npm run build\"",
    "lint": "eslint src test packages",
    "dbg:neutrino:dev": "neutrino --inspect --mode development",
    "dbg:neutrino:prod": "neutrino --inspect --mode production",
    "start": "npm-run-all --parallel build:components:dev webpackdev devserver",
    "webpackdev": "webpack-dev-server --mode development",
    "devserver": "npm run build && PORT=5001 nodemon -w './src/server/**/*' -w './packages/*/src/server/**/*' ./src/server/index.js",
    "build": "webpack --mode production",
    "postbuild": "mkdir -p build && mkdir -p dist && cp -r build/* dist/",
    "build:html": "npm run build && BUILD_TYPE=html npm run build",
    "test:e2e": "jest --runInBand --config test/ui/jest.config.js",
    "test:unit": "jest --runInBand --config test/server/jest.config.js",
    "test": "npm run build:html && JEST_SERVE=1 run-s test:*",
    "watch:test:e2e": "NODE_WATCH=1 jest --watch --runInBand --config test/ui/jest.config.js",
    "watch:test:unit": "jest --runInBand --watch --config test/server/jest.config.js",
    "postinstall": "npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zeropaper/ezt.git"
  },
  "author": {
    "name": "Valentin Vago",
    "email": "zeropaper@irata.ch",
    "url": "http://irata.graphics/"
  },
  "license": "MIT",
